{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "outDir": "../out-tsc/app",
    "baseUrl": "./",
    "module": "es2020",
    "isolatedModules": true,
    "types": [
      "node",
      "express"
    ],
    "paths": {
      "ng-zorro-antd": [
        "../components/ng-zorro-antd.module.ts"
      ],
      "ng-zorro-antd/*": [
        "../components/*"
      ]
    }
  },
  "exclude": [
    "test.ts",
    "**/*.spec.ts"
  ],
  "angularCompilerOptions": {
    "strictInjectionParameters": true,
    "strictInputAccessModifiers": true,
    "strictTemplates": true
  }
}
