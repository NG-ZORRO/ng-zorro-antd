    "@types/parse5": "^5.0.3",
    "@types/shelljs": "^0.8.8",
    "antd-theme-generator": "1.2.2",
    "chalk": "^4.1.0",
    "classlist.js": "^1.1.20150312",
    "clean-css": "~4.2.3",
    "codecov": "^3.8.1",
    "codelyzer": "^6.0.1",
    "codesandbox": "^2.2.1",
    "conventional-changelog-cli": "^2.1.1",
    "d3": "^6.3.1",
    "dagre": "^0.8.5",
    "dagre-compound": "0.0.4",
    "detect-port": "^1.3.0",
    "fs-extra": "^9.0.1",
    "gulp": "^4.0.2",
    "gulp-clean": "^0.4.0",
    "html-minifier-terser": "^5.1.1",
