    "readline-sync": "^1.4.10",
    "remark": "^13.0.0",
    "resolve-bin": "^0.4.0",
    "rxjs": "~6.6.3",
    "shelljs": "^0.8.4",
    "sitemap": "^4.1.1",
    "terser": "^5.5.1",
    "ts-node": "~9.1.1",
    "tslib": "^2.0.3",
    "tslint": "~6.1.3",
    "typescript": "~4.1.2",
    "yaml-front-matter": "^4.1.1",
    "zone.js": "~0.11.3"
  },
  "lint-staged": {
    "components/**/*.{html,ts}": [
      "./node_modules/.bin/prettier --write",
      "git add"
    ],
    "components/**/!(testing)/!(polyfills).ts": [
      "tslint --project components/tsconfig.lib.json -c tslint.json --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  }
}
